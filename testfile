@Week shares(startdate#2023-05-01) {
    @Day Mon mods(startdate# + 0d);
    @Day Tue mods(startdate# + 1d);
    @Day Wed mods(startdate# + 2d);
    @Day Thu mods(startdate# + 3d);
    @Day Fri mods(startdate# + 4d);
    @Day Sat mods(startdate# + 5d);
    @Day Sun mods(startdate# + 6d);
};

@Books {
    @Series 
        title#"Harry Potter" 
        shares( 
            author#"J. K. Rowling"
            genres#[fiction fantasy magic]
        )
    {
        @Book title#"Harry Potter and the Philosopher's Stone"
              status#Read;  
    };
    @Book title#"The Catcher in the Rye" 
          author#"J.D. Salinger" 
          pages#277 
          status#Read 
          genres#[fiction coming-of-age];
    @Book title#"To Kill a Mockingbird" 
          author#"Harper Lee" pages#324 
          status#Read 
          genres#[fiction classic social-issues];
    @Book title#"Dune" 
          author#"Frank Herbert" 
          pages#896 
          status#Reading 
          genres#[science-fiction fantasy adventure];
    @Book title#"1984" 
